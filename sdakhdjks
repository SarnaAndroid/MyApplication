[32m+[m[32m    @Override[m
[32m+[m[32m    public void onClick(View view) {[m
[32m+[m
[32m+[m[32m        switch (view.getId()) {[m
[32m+[m[32m            case R.id.btn_register:[m
[32m+[m[32m                name = etName.getText().toString();[m
[32m+[m[32m                email = etEmail.getText().toString();[m
[32m+[m[32m                username = etUsername.getText().toString();[m
[32m+[m[32m                password = etPassword.getText().toString();[m
[32m+[m[32m                confirmPassword = etConfirmPassword.getText().toString();[m
[32m+[m
[32m+[m[32m                if (isDataValidate()){[m
[32m+[m[32m                    Toast.makeText(this, "Congrats You are Registered", Toast.LENGTH_LONG).show();[m
[32m+[m[32m                    Intent intent = new Intent(this, LoginActivity.class);[m
