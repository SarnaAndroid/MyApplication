[32m+[m[32m                Intent intent = new Intent(this, LoginActivity.class);[m
[32m+[m[32m                startActivity(intent);[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private boolean isDataValidate() {[m
[32m+[m[32m        if (name == null || name.equals("")) {[m
[32m+[m[32m            etName.setError("Fill Correct Name");[m
[32m+[m
[32m+[m[32m        } else if (email == null || email.equals("")) {[m
[32m+[m[32m            etEmail.setError("Fill Correct Email");[m
[32m+[m
[32m+[m[32m        } else if (username == null || username.equals("")) {[m
[32m+[m[32m            etUsername.setError("Fill Correct Username");[m
[32m+[m
[32m+[m[32m        } else if (password == null || password.equals("")) {[m
[32m+[m[32m            etPassword.setError("Fill Correct Password");[m
[32m+[m
[32m+[m[32m        } else if (confirmPassword == null || confirmPassword.equals("")) {[m
[32m+[m[32m            etConfirmPassword.setError("Fill Correct Password");[m
[32m+[m
[32m+[m[32m        } else if (!confirmPassword.equals(password)){[m
[32m+[m[32m            etConfirmPassword.setError("Confirm Password not match");[m
[32m+[m
[32m+[m[32m        }else{[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
