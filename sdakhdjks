[32m+[m[32m        setContentView(R.layout.activity_dashboard);[m
[32m+[m
[32m+[m[32m        toolbar = findViewById(R.id.toolbar);[m
[32m+[m[32m        setSupportActionBar(toolbar);[m
[32m+[m
[32m+[m[32m        initView();[m
[32m+[m[32m        initViewsWithData();[m
[32m+[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void setUpTabBar() {[m
[32m+[m
[32m+[m[32m        PagerAdapter pagerAdapter=new FragmentAdapter(getSupportFragmentManager());[m
[32m+[m[32m        viewPager.setAdapter(pagerAdapter);[m
[32m+[m
[32m+[m[32m        tabLayout.setupWithViewPager(viewPager);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void initView() {[m
[32m+[m[32m        tabLayout = findViewById(R.id.tbl_pages);[m
[32m+[m[32m        viewPager = findViewById(R.id.vp_pages);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void initViewsWithData() {[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        setUpDrawerAndToolBar();[m
[32m+[m[32m        setUpTabBar();[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m    private void setUpDrawerAndToolBar() {[m
[32m+[m[32m        Toolbar toolbar = (Toolbar) findViewById(R.id.toolbar);[m
[32m+[m[32m        setSupportActionBar(toolbar);[m
[32m+[m
[32m+[m[32m        DrawerLayout drawer = (DrawerLayout) findViewById(R.id.drawer_layout);[m
[32m+[m[32m        ActionBarDrawerToggle toggle = new ActionBarDrawerToggle([m
[32m+[m[32m                this, drawer, toolbar, R.string.navigation_drawer_open, R.string.navigation_drawer_close);[m
[32m+[m[32m        drawer.addDrawerListener(toggle);[m
[32m+[m[32m        toggle.syncState();[m
[32m+[m
[32m+[m[32m        NavigationView navigationView = (NavigationView) findViewById(R.id.nav_view);[m
[32m+[m[32m        navigationView.setNavigationItemSelectedListener(this);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onBackPressed() {[m
[32m+[m[32m        DrawerLayout drawer = (DrawerLayout) findViewById(R.id.drawer_layout);[m
[32m+[m[32m        if (drawer.isDrawerOpen(GravityCompat.START)) {[m
[32m+[m[32m            drawer.closeDrawer(GravityCompat.START);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            super.onBackPressed();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onCreateOptionsMenu(Menu menu) {[m
