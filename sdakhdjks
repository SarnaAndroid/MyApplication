[32m+[m[32mpublic class DashboardActivity extends AppCompatActivity[m
[32m+[m[32m        implements   NavigationView.OnNavigationItemSelectedListener {[m
[32m+[m
[32m+[m
[32m+[m[32m    private TabLayout tabLayout;[m
[32m+[m
[32m+[m[32m    //This is our viewPager[m
[32m+[m[32m    private ViewPager viewPager;[m
[32m+[m[32m    private Toolbar toolbar;[m
