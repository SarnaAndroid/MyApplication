[32m+[m[32m        etPassword = findViewById(R.id.et_password);[m
[32m+[m[32m        etConfirmPassword = findViewById(R.id.et_confirm_password);[m
[32m+[m[32m        btnRegister = findViewById(R.id.btn_register);[m
[32m+[m[32m        tvLogin = findViewById(R.id.tv_login);[m
[32m+[m[32m    }[m
